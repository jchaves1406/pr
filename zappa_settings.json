{
    "dev": {
        "app_function": "lambda_csv.save_csv",
        "keep_warm": false,
        "role_name": "LabRole",
        "manage_roles": false,
        "apigateway_enabled": false,
        "project_name": "zappa",
        "runtime": "python3.8",
        "s3_bucket": "landing-casas-xxx",
        "aws_region": "us-east-1",
        "events": [
            {
                "function": "lambda_csv.save_csv",
                "event_source": {
                    "arn": "arn:aws:s3:::landing-casas-xxx",
                    "events": [
                        "s3:ObjectCreated:*"
                    ]
                }
            }
        ],
        "environment_variables": {
            "BUCKET_NAME": "casas-final-xxx"
        },
        "slim_handler": true
    }
}